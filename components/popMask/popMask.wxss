.popup-mask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 100;
    overflow: hidden;
    background: rgba(51, 51, 51, 0.4);
    opacity: 0;
}
.bottom-34 {
    bottom: 34px !important;
} 
.popup-mask-box {
    position: fixed;
    left: 0;
    right: 0;
    overflow: hidden;
    max-height: 100%;
    background: #f1f4f9;
    border-radius: 40rpx 40rpx 0 0;
    z-index: 100;
}
.popup-mask-bottom {
    bottom: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
}
.popup-mask-box .popup-body {
    position: relative;
    overflow: scroll;
    -webkit-overflow-scrolling: touch;
}
.popup-mask-box .popup-body.bg-fff {
    background-color: #fff !important;
}
.scroll-content {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
.scroll-content.bottom-1 {
    bottom: -1px;
} 
.popup-mask-box .popup-footer,
.popup-mask-box .popup-footers {
    padding: 20rpx 35rpx;
    background: #fff;
    box-shadow: 0 0 58rpx 0 rgba(155, 155, 155, 0.18);
}
.popup-mask-box .popup-footers {
    padding: 20rpx 20rpx;
}
.popup-mask-box .popup-btn {
    height: 84rpx;
    line-height: 84rpx;
    text-align: center;
    color: #fff;
    border-radius: 84rpx;
    background: #0dd66c;
    font-size: 36rpx;
    font-weight: bold;
}
.popup-mask-box .popup-btn.first-btn {
    margin-right: 20rpx;
    color: #666;
    background: #fff;
    font-weight: normal;
}
.thin-border-all:before {
    border-radius: 84rpx;
}
.close {
    position: absolute;
    top: 0;
    right: 0;
    width: 88rpx;
    height: 108rpx;
    line-height: 108rpx;
    text-align: center;
    z-index: 10;
}
.icon-cross {
    display: inline-block;
    width: 14px;
    height: 14px;
    background: url('https://file.40017.cn/trainwechat/product/miniapp/common/icon-1th.png?v=1') no-repeat;
    background-size: 320px; 
    background-position: -10.5px -113.5px;
}
.header {
    background: #fff;
    line-height: 1;
}
.header .header-title {
    padding: 35rpx 0 35rpx 30rpx;
    font-size: 40rpx;
    color: #333 !important;
}
.blod {
    font-weight:bold;
}
.header .header-subtitle {
    padding-top: 20rpx;
    font-size: 28rpx;
}
.tips {
    padding: 20rpx 30rpx;
    background: #fefced;
    color: #f76a23;
    font-size: 24rpx;
    line-height: 34rpx;
}
/* flex容器属性 开始 */
.flex {
    display: -webkit-box; 
    display: -webkit-flex; 
    display: flex; 
    width: 100%;
}
.flex-column {
    height: 100%;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    flex-direction: column;
}
/* flex项目属性 开始 */
.flex1 {-webkit-box-flex: 1.0; -webkit-flex: 1; flex: 1;}
/* flex项目属性 结束 */
.thin-border-all,
.thin-border-top,
.thin-border-right,
.thin-border-bottom,
.thin-border-left {position: relative;}
.thin-border-all:before,
.thin-border-top:before,
.thin-border-right:before,
.thin-border-bottom:before,
.thin-border-left:before {content: ''; position: absolute; z-index: 1; -webkit-box-sizing: border-box; box-sizing: border-box; border: 0 solid #eaeaea;}
.thin-border-all:before,
.thin-border-top:before,
.thin-border-left:before {top: 0; left: 0; -webkit-transform-origin: left top; transform-origin: left top;}
.thin-border-right:before {top: 0; right: 0; -webkit-transform-origin: right top; transform-origin: right top;}
.thin-border-bottom:before {bottom: 0; left: 0; -webkit-transform-origin: left bottom; transform-origin: left bottom;}
.thin-border-all:before {width:200%; height: 200%; -webkit-transform: scale(.5); transform: scale(.5);}
.thin-border-top:before,
.thin-border-bottom:before {width:100%; height: 0; -webkit-transform: scaleY(.5); transform: scaleY(.5);}
.thin-border-right:before,
.thin-border-left:before {width:0; height: 100%; -webkit-transform: scaleX(.5); transform: scaleX(.5);}
.thin-border-all:before {border-width: 1px;}
.thin-border-top:before {border-top-width: 1px;}
.thin-border-right:before {border-right-width: 1px;}
.thin-border-bottom:before {border-bottom-width: 1px;}
.thin-border-left:before {border-left-width: 1px;}
.thin-border-all {position: relative; z-index: 1;}