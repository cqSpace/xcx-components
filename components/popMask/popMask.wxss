.popup-mask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 100;
    overflow: hidden;
    background: rgba(51, 51, 51, .7);
    opacity: 0;
    -webkit-transition: opacity 200ms linear;
    transition: opacity 200ms linear;
}
.bottom-34 {
    bottom: 34px !important;
}
.popup-mask-box {
    position: fixed;
    left: 0;
    right: 0;
    overflow: hidden;
    max-height: 100%;
    background: #f1f4f9;
    border-radius: 30rpx 30rpx 0 0;
    z-index: 100;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
.popup-mask-bottom {
    bottom: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
}
.popup-mask-box .content-box {
    position: relative;
    height: 100%;
    background: #f1f4f9;
    border-radius: 30rpx 30rpx 0 0;
    overflow: hidden;
}
.popup-mask-box .popup-body {
    position: relative;
    height: 100%;
    overflow: hidden;
}
.popup-mask-box .popup-body.bg-fff {
    background-color: #fff !important;
}
.scroll-content {
    position: relative;
    height: 100%;
}
.scroll-content .take-place {
    margin-top: -1px;
    padding-top: 1px;
}
.scroll-content-box {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
}
.popup-mask-box .popup-footer,
.popup-mask-box .popup-footers {
    position: relative;
    padding: 20rpx 35rpx;
    background: #fff;
    /* box-shadow: 0 0 58rpx 0 rgba(155, 155, 155, 0.18); */
}
.popup-mask-box .popup-footers {
    padding: 20rpx 20rpx;
}
.popup-mask-box .popup-btn {
    height: 84rpx;
    line-height: 84rpx;
    text-align: center;
    color: #fff;
    border-radius: 84rpx;
    background: #0dd66c;
    font-size: 36rpx;
    font-weight: bold;
}
.popup-mask-box .popup-btn.first-btn {
    color: #666;
    background: #fff;
    font-weight: normal;
}
.popup-mask-box .popup-btn:not(:first-child) {
    margin-left: 20rpx;
}
.thin-border-all:before {
    border-radius: 84rpx;
}
.close {
    position: absolute;
    top: 0;
    right: 0;
    width: 88rpx;
    height: 108rpx;
    z-index: 10;
}
.icon-cross {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 28rpx;
    height: 28rpx;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.icon-cross:before {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    width: 4rpx;
    height: 40rpx;
    background: #d3d3d3;
    border-radius: 2rpx;
    -webkit-transform: translate(-50%, -50%) rotate(45deg);
    transform: translate(-50%, -50%) rotate(45deg);
}
.icon-cross:after {
    content: "";
    position: absolute;
    top: 50%;
    right: 50%;
    width: 4rpx;
    height: 40rpx;
    background: #d3d3d3;
    border-radius: 2rpx;
    -webkit-transform: translate(50%, -50%) rotate(-45deg);
    transform: translate(50%, -50%) rotate(-45deg);
}
