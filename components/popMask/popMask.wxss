/* flex容器属性 开始 */
.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
}
.flex-column {
    height: 100%;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    flex-direction: column;
}
/* flex项目属性 开始 */
.flex1 {-webkit-box-flex: 1.0; -webkit-flex: 1; flex: 1;}
/* flex项目属性 结束 */
.thin-border-all,
.thin-border-top,
.thin-border-right,
.thin-border-bottom,
.thin-border-left {position: relative;}
.thin-border-all:before,
.thin-border-top:before,
.thin-border-right:before,
.thin-border-bottom:before,
.thin-border-left:before {content: ''; position: absolute; z-index: 1; -webkit-box-sizing: border-box; box-sizing: border-box; border: 0 solid #eaeaea;}
.thin-border-all:before,
.thin-border-top:before,
.thin-border-left:before {top: 0; left: 0; -webkit-transform-origin: left top; transform-origin: left top;}
.thin-border-right:before {top: 0; right: 0; -webkit-transform-origin: right top; transform-origin: right top;}
.thin-border-bottom:before {bottom: 0; left: 0; -webkit-transform-origin: left bottom; transform-origin: left bottom;}
.thin-border-all:before {width:200%; height: 200%; -webkit-transform: scale(.5); transform: scale(.5);}
.thin-border-top:before,
.thin-border-bottom:before {width:100%; height: 0; -webkit-transform: scaleY(.5); transform: scaleY(.5);}
.thin-border-right:before,
.thin-border-left:before {width:0; height: 100%; -webkit-transform: scaleX(.5); transform: scaleX(.5);}
.thin-border-all:before {border-width: 1px;}
.thin-border-top:before {border-top-width: 1px;}
.thin-border-right:before {border-right-width: 1px;}
.thin-border-bottom:before {border-bottom-width: 1px;}
.thin-border-left:before {border-left-width: 1px;}
.thin-border-all {position: relative; z-index: 1;}
